@using Umbraco.Cms.Web.Common.PublishedModels;
@inherits Umbraco.Cms.Web.Common.Views.UmbracoViewPage<Umbraco.Cms.Core.Models.Blocks.BlockListItem>;
@using ContentModels = Umbraco.Cms.Web.Common.PublishedModels;
@using Umbraco.Cms.Core.Models

@{
         var content = (ContentModels.AboutTestimonial)Model.Content;
    
}
        <div class="item">
            <p>
                @content.Value("aboutTestimonialDesc")
            </p>
            <div class="info">
                <div class="img">
                    <div class="img-box">
                        <img src="img/clients/1.jpg" alt="">
                    </div>
                </div>
                <div class="cont">
                    <div class="author">
                        <h6 class="author-name custom-font">@content.Value("aboutTestimonialName")</h6>
                        <span class="author-details">@content.Value("aboutTestimonialPostion")</span>
                    </div>
                </div>
            </div>
        </div>
        		    
        